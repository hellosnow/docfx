<header class="header" id="overview">
  {{#conceptual}}
  <div>{{{conceptual}}}</div>
  {{/conceptual}}
  {{^conceptual}}
  <h1 id="{{htmlId}}" data-uid="{{uid}}">{{name}}</h1>
  {{/conceptual}}
</header>
{{#children}}
<header class="header" id="{{operationId}}">
  {{#conceptual}}
  <div>{{{conceptual}}}</div>
  {{/conceptual}}
  {{^conceptual}}
  <h3 id="{{htmlId}}" data-uid="{{uid}}">{{{summary}}}</h3>
  <div>
    {{{description}}}
  </div>
  {{/conceptual}}
</header>
<div class="level">
  <section class="primary">
    <h4>Request</h4>
    <div>
      <!--todo: change language according to highlight.js-->
      <pre><code class="lang-cplusplus">{{operation}} {{path}}</code></pre>
    </div>
    {{#parameters.0}}
    <h4>Properties</h4>
    <ul class="member-details">
      {{#parameters}}
      <li>
        <ol class="meta">
          <li>
            <code>{{name}}</code>
            {{#required}}
            <em>(required)</em>
            {{/required}}
          </li>
          <li>
            <!--todo: complex type-->
            <code>{{type}}</code>
          </li>
        </ol>
        <section class="description">
          {{description}}
        </section>
      </li>
      {{/parameters}}
    </ul>
    {{/parameters.0}}
    {{#responses.0}}
    <h4>Returns</h4>
    <h6>HTTP RESPONSE CODES</h6>
    <ul class="member-details">
      {{#responses}}
      <li>
        <ol class="meta">
          <li>
            <code>{{statusCode}}</code>
          </li>
        </ol>
        <section class="description">
          {{description}}
        </section>
      </li>
      {{/responses}}
    </ul>
    {{/responses.0}}
  </section>
  <section class="secondary">
    <!--todo: swagger url-->
    <DIV class="api-example"
    {{#tryFeature}}
    data-api-interactive
    {{/tryFeature}}
    data-api-example-endpoint="{{_mockServerUrl}}"
    data-api-swagger-url=""
    data-api-operation-id="{{operationId}}"/>
  </section>
</div>
{{/children}}
